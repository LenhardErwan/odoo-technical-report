<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<record id='tr_block_action' model='ir.actions.act_window'>
		<field name="name">Technical Reports</field>
		<field name="res_model">tr.block</field>
		<field name="view_mode">tree,form</field>
	</record>

	<record id="tr_block_view_form" model="ir.ui.view">
		<field name="name">Block Form</field>
		<field name="model">tr.block</field>
		<field name="arch" type="xml">
			<form>
				<group>
					<field name="order" />
					<field name="image" />
					<field name="text" />
				</group>
			</form>
		</field>
	</record>

	<record id="tr_block_view_tree" model="ir.ui.view">
		<field name="name">Block List</field>
		<field name="model">tr.block</field>
		<field name="arch" type="xml">
			<tree>
				<field name="order" />
				<field name="text" />
				<field name="image_exist" />
			</tree>
		</field>
	</record>
</odoo>